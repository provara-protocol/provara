{
  "version": "1.0",
  "description": "Provara RFC 8785 Canonical JSON Conformance Suite",
  "vectors": [
    {
      "id": "key_ordering_01",
      "description": "Basic lexicographic key sorting",
      "input": {
        "z": 1,
        "a": 2,
        "m": 3
      },
      "expected_hex": "7b2261223a322c226d223a332c227a223a317d"
    },
    {
      "id": "key_ordering_unicode",
      "description": "Unicode key sorting (code point order)",
      "input": {
        "\u00a3": 1,
        "a": 2,
        "\u20ac": 3,
        "z": 4
      },
      "expected_hex": "7b2261223a322c227a223a342c22c2a3223a312c22e282ac223a337d"
    },
    {
      "id": "empty_containers_01",
      "description": "Empty object and array are preserved",
      "input": {
        "empty_obj": {},
        "empty_arr": []
      },
      "expected_hex": "7b22656d7074795f617272223a5b5d2c22656d7074795f6f626a223a7b7d7d"
    },
    {
      "id": "nested_structure_01",
      "description": "Deeply nested objects and arrays",
      "input": {
        "a": [
          1,
          {
            "b": 2
          },
          [
            3,
            4
          ]
        ],
        "c": {
          "d": {
            "e": 5
          }
        }
      },
      "expected_hex": "7b2261223a5b312c7b2262223a327d2c5b332c345d5d2c2263223a7b2264223a7b2265223a357d7d7d"
    },
    {
      "id": "null_handling_01",
      "description": "Null values MUST be preserved",
      "input": {
        "a": null,
        "b": {
          "c": null
        }
      },
      "expected_hex": "7b2261223a6e756c6c2c2262223a7b2263223a6e756c6c7d7d"
    },
    {
      "id": "string_escapes_01",
      "description": "Minimal escapes in strings",
      "input": {
        "s": "quote: \", backslash: \\, tab: \t, newline: \n"
      },
      "expected_hex": "7b2273223a2271756f74653a205c222c206261636b736c6173683a205c5c2c207461623a205c742c206e65776c696e653a205c6e227d"
    },
    {
      "id": "unicode_string_distinct_01",
      "description": "NFC and NFD strings stay byte-distinct",
      "input": {
        "nfc": "\u00e9",
        "nfd": "e\u0301"
      },
      "expected_hex": "7b226e6663223a22c3a9222c226e6664223a2265cc81227d"
    },
    {
      "id": "number_formatting_01",
      "description": "Finite numbers in canonical JSON encoding",
      "input": {
        "int": 10,
        "neg_int": -7,
        "frac": 0.125,
        "big": 1000000.5
      },
      "expected_hex": "7b22626967223a313030303030302e352c2266726163223a302e3132352c22696e74223a31302c226e65675f696e74223a2d377d"
    },
    {
      "id": "number_formatting_minus_zero",
      "description": "Minus zero preserved by current Python canonicalizer",
      "input": {
        "minus_zero": -0.0,
        "zero": 0.0
      },
      "expected_hex": "7b226d696e75735f7a65726f223a2d302e302c227a65726f223a302e307d"
    },
    {
      "id": "array_mixed_types_01",
      "description": "Mixed JSON primitives in array order",
      "input": {
        "arr": [
          null,
          true,
          false,
          0,
          "x",
          {
            "k": "v"
          }
        ]
      },
      "expected_hex": "7b22617272223a5b6e756c6c2c747275652c66616c73652c302c2278222c7b226b223a2276227d5d7d"
    },
    {
      "id": "key_prefix_order_01",
      "description": "Key ordering with shared prefixes",
      "input": {
        "a": 1,
        "a0": 2,
        "a_": 3,
        "aa": 4
      },
      "expected_hex": "7b2261223a312c226130223a322c22615f223a332c226161223a347d"
    },
    {
      "id": "boolean_and_null_01",
      "description": "Boolean and null fields canonicalization",
      "input": {
        "t": true,
        "f": false,
        "n": null
      },
      "expected_hex": "7b2266223a66616c73652c226e223a6e756c6c2c2274223a747275657d"
    }
  ]
}
